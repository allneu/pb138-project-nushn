{
  "name": "backend",
  "version": "0.1.0",
  "description": "Backend part of the Nushn project.",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "watch": "nodemon src/index.ts",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "create-db": "cd prisma && docker compose up --detach && npx prisma migrate dev --name init",
    "seed": "prisma migrate reset --force && ts-node src/seed/seed.ts"
  },
